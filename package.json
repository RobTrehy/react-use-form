    "@vitejs/plugin-react": "^1.3.0",
    "@vitest/ui": "latest",
    "c8": "^7.11.0",
    "jsdom": "^19.0.0",
    "path": "^0.12.7",
    "react-test-renderer": "^16.9.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
